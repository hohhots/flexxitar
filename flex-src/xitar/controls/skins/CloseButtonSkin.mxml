	<!-- states -->
	<s:states>
		<s:State name="disabled" />
		<s:State name="up" />
		<s:State name="down" />
		<s:State name="over" />
	</s:states>
	
	<!-- SkinParts
	name=contentGroup, type=spark.components.Group, required=false
	-->

	<s:Rect>
	</s:Rect>
	
	<!-- layer 1: border -->
	<s:Rect>
	</s:Rect>
	
	<s:Rect>
	</s:Rect>
	
	<s:Group id="contentGroup" left="0" right="0" top="0" bottom="0">
		<s:BitmapImage source="@Embed(source='../../images/exit.png')" excludeFrom="over,down" />
		<s:BitmapImage source="@Embed(source='../../images/exitover.png')" includeIn="over" />
		<s:BitmapImage source="@Embed(source='../../images/exitclick.png')" includeIn="down" />
	</s:Group>
	
